import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Таблица а):");
        for (int i = 1; i <= 5; i++) {
            for (int j = 0; j < i; j++) {
                System.out.print(i + " ");
            }
            System.out.println();
        }

        System.out.println();

        System.out.println("Таблица б):");
        for (int i = 5; i <= 9; i++) {
            for (int j = 0; j < (10 - i); j++) {
                System.out.print(i + " ");
            }
            System.out.println();
        }

        System.out.println();

        System.out.println("Таблица в):");
        for (int i = 1; i <= 5; i++) {
            int num = i * 10;
            for (int j = 0; j < i; j++) {
                System.out.print(num + " ");
            }
            System.out.println();
        }

        System.out.println();

        System.out.println("Таблица г):");
        for (int i = 5; i <= 25; i += 5) {
            for (int j = 0; j < (10 - (i / 5)); j++) {
                System.out.print(i + " ");
            }
            System.out.println();
        }
    }
}


