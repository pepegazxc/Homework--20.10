import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.println("Таблица а):");
        for (int i = 0; i < 6; i++) {
            for (int j = 0; j < 6; j++) {
                System.out.print("5 ");
            }
            System.out.println();
        }

        System.out.println();

        System.out.println("Таблица б):");
        for (int i = 0; i < 4; i++) {
            for (int j = 1; j <= 10; j++) {
                System.out.print(j + " ");
            }
            System.out.println();
        }

        System.out.println();

        System.out.println("Таблица в):");
        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 10; j++) {
                System.out.print((i * 10 + j + 41) + " ");
            }
            System.out.println();
        }
    }
}
