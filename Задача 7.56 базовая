import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String city = sc.nextLine();
        int distance = -1;

        if (city.equals("Санкт-Петербург")){
            distance = 635;
        } else if (city.equals("Нижний Новгород")){
            distance = 400;
        } else if (city.equals("Казань")) {
            distance = 820;
        }
        
        if (distance != -1) {
            System.out.println("Расстояние от Москвы до " + city + " составляет " + distance + " км.");
        }else {
            System.out.println("Город не найден.");
        }
    }
}
